    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-react-css-modules": "^2.2.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
