{
	
	"presets": [
		"stage-1",
		"react"
	],

	"plugins": [
		"dev-expression"
	],

	"env": {
		"cjs": {
			"presets": [
				[ "es2015", {
					"loose": true,
					"modules": "commonjs"
				}]
			],
			"plugins": [
				"add-module-exports"
			]
		},
		"development": {
			"plugins": [
				"react-hot-loader/babel"
			]
		}
	}

}