{
	"presets": [
		"stage-1",
		"react"
	],

	"plugins": [
		"dev-expression"
	],

	"env": {
		
		"cjs": {
			"presets": [
				[ "es2015", {
					"loose": true,
					"modules": "commonjs"
				}]
			],
			"plugins": [
				"add-module-exports"
			]
		},

		"es": {
			"presets": [
				[ "es2015", {
					"loose": true,
					"modules": false
				}]
			]
		},

		"development": {
			"plugins": [
				"react-hot-loader/babel"
			]
		}

	}
}